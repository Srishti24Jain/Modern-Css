    <div class="bg-white shadow px-4 md:px-10 pt-4 md:pt-7 pb-5 h-full w-full" style="
    padding-bottom: 0px;
    padding-right: 0px;
    padding-top: 0px;
    height: 763px;
    width: 1024px;
    padding-left: 40px;
">
        <div class="px-4 md:px-10 py-4 md:py-7 bg-gray-100 rounded-tl-lg rounded-tr-lg">
            <div class="max-w-9xl h-full msm:px-6  ">
                <div class="flex justify-end items-center">
                    <div class="relative pb-4">
                        <div class="absolute top-4 left-3 "> <i class="fa fa-search text-gray-400 z-20 hover:text-gray-500 "></i> </div> <input type="text" class="h-14 w-96 pl-10 pr-20 rounded-lg z-0 focus:shadow focus:outline-none" placeholder="Search anything...">
                        <div class="absolute top-2 right-2"> <button class="h-10 w-20 text-white rounded-lg bg-indigo-600 hover:bg-red-600">Search</button> </div>
                    </div>
                </div>
                <div class="sm:flex items-center justify-between">
                    <p tabindex="0" class="focus:outline-none text-base sm:text-lg text-blue-500 pt-6">Employee </p>
                    <p tabindex="0" class="focus:outline-none text-base sm:text-lg text-gray-500 pt-6">Contractor</p>
                    <div>
                        <button class="focus:ring-2 focus:ring-offset-2 focus:ring-indigo-600 inline-flex sm:ml-3 mt-4 sm:mt-0 items-start justify-start px-6 py-3 bg-indigo-700 hover:bg-indigo-600 focus:outline-none rounded " onclick="location.href='/form'">
                        <p class="text-sm font-medium leading-none text-white">+Add Employee</p>
                        </button>
                    </div>
                </div>
            </div>
            <form class="w-full max-w-8xl h-full px-6 bg-white-100 " action="/form" method="POST">
                <div class="flex flex-wrap -mx-3 mb-6 ">
                    <div class="w-full py-4 text-bold ">
                        <p>Basic Information</p>
                    </div>
                    <div class="w-full md:w-2/3 px-3 mb-6 md:mb-0 ">
                        <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-first-name ">
        First Name: *
      </label>
                        <input class="appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white " id="grid-first-name " name="fName" type="text
                    " placeholder="First Name " Required>
                        <!-- <p class="text-red-500 text-xs italic ">Please fill out this field.</p> -->
                    </div>
                    <div class="w-full md:w-1/3 px-3 ">
                        <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-last-name ">
        Middle Name:
      </label>
                        <input class="appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 leading-tight focus:outline-none focus:bg-white focus:border-gray-500 " id="grid-last-name " name="lName" type="text" placeholder="Middle">
                    </div>
                </div>
                <div class="flex flex-wrap -mx-3 mb-6 ">
                    <div class="w-full px-3 ">
                        <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-password ">
      Password: *
      </label>
                        <input class="appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white focus:border-gray-500 " id="grid-password " name="pass" type="password" placeholder="Password "
                            Required>
                    </div>
                </div>
                <div class="flex flex-wrap -mx-3 mb-6 ">
                    <div class="w-full md:w-2/3 px-3 mb-6 md:mb-0 ">
                        <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-city ">
        Employee ID: *
      </label>
                        <input class="appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 leading-tight focus:outline-none focus:bg-white focus:border-gray-500 " id="grid-city " type="text " name="emp" placeholder="AT-4336 " Required>
                    </div>
                    <div class="w-full md:w-1/3 px-3 mb-6 md:mb-0 ">
                        <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-state ">
        Gender:
      </label>
                        <div class="relative ">
                            <select class="block appearance-none w-full bg-gray-200 border border-gray-200 text-gray-700 py-3 px-4 pr-8 rounded leading-tight focus:outline-none focus:bg-white focus:border-gray-500 " name="gen" id="grid-state ">
          <option>Male</option>
          <option>Female</option>
          <option>Others</option>
        </select>
                            <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700 ">
                                <svg class="fill-current h-4 w-4 " xmlns="http://www.w3.org/2000/svg " viewBox="0 0 20 20 "><path d="M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z "/></svg>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="flex flex-wrap -mx-3 mb-6 ">
                    <div class="w-full px-3 ">
                        <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-state ">
        Start Date: *
      </label>
                        <input class="appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white focus:border-gray-500 " id="grid-state " type="date" name="dat" placeholder="12-11-1999 "
                            Required>
                    </div>
                </div>
                <div class="flex flex-wrap -mx-3 mb-2 ">
                    <div class="w-full md:w-2/3 px-3 mb-6 md:mb-0 ">
                        <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-state ">
        Department:
      </label>
                        <div class="relative ">
                            <select class="block appearance-none w-full bg-gray-200 border border-gray-200 text-gray-700 py-3 px-4 pr-8 rounded leading-tight focus:outline-none focus:bg-white focus:border-gray-500 " name="dep" id="grid-state ">
          <option>Design and Media</option>
          <option>EEE</option>
          <option>ME</option>
        </select>
                            <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700 ">
                                <svg class="fill-current h-4 w-4 " xmlns="http://www.w3.org/2000/svg " viewBox="0 0 20 20 "><path d="M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z "/></svg>
                            </div>
                        </div>
                    </div>
                    <div class="w-full md:w-1/3 px-3 mb-6 md:mb-0 ">
                        <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-state ">
        Designation:
      </label>
                        <div class="relative ">
                            <select class="block appearance-none w-full bg-gray-200 border border-gray-200 text-gray-700 py-3 px-4 pr-8 rounded leading-tight focus:outline-none focus:bg-white focus:border-gray-500 " name="des" id="grid-state ">
          <option>UI/UX Designer</option>
          <option>Developer</option>
          <option>Android Developer</option>
        </select>
                            <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700 ">
                                <svg class="fill-current h-4 w-4 " xmlns="http://www.w3.org/2000/svg " viewBox="0 0 20 20 "><path d="M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z "/></svg>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="flex flex-wrap -mx-3 mb-6 ">
                    <div class="w-full px-3 ">
                        <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-password ">
        Work Email: *
      </label>
                        <input class="appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white focus:border-gray-500 " name="email" id="grid-state" type="email" placeholder="seri@gmail.com "
                            Required>
                        <p class="text-gray-600 text-xs italic ">Make it as long and as crazy as you'd like</p>
                    </div>
                </div>
                <div class="flex flex-wrap -mx-3 mb-6 ">
                    <div class="w-full px-3 ">
                        <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-password ">
        Work Address: *
      </label>
                        <input class="appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white focus:border-gray-500 " id="grid-password " name="add" type="text " placeholder="Address "
                            Required>
                        <p class="text-gray-600 text-xs italic ">Make it as long and as crazy as you'd like</p>
                    </div>
                </div>
                <div class="flex flex-wrap -mx-3 mb-2 ">
                    <div class="w-full md:w-2/3 px-3 mb-6 md:mb-0 ">
                        <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-state ">
        Select/Provice:
      </label>
                        <div class="relative ">
                            <select class="block appearance-none w-full bg-gray-200 border border-gray-200 text-gray-700 py-3 px-4 pr-8 rounded leading-tight focus:outline-none focus:bg-white focus:border-gray-500 " name="pro" id=" grid-state ">
          <option>Delhi</option>
          <option>Gurugram</option>
          <option>Banglore</option>
        </select>
                            <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700 ">
                                <svg class="fill-current h-4 w-4 " xmlns="http://www.w3.org/2000/svg " viewBox="0 0 20 20 "><path d="M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z "/></svg>
                            </div>
                        </div>
                    </div>
                    <div class="w-full md:w-1/3 px-3 mb-6 md:mb-0 ">
                        <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-city ">
        City:*
      </label>
                        <input class="appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 leading-tight focus:outline-none focus:bg-white focus:border-gray-500 " id="grid-city " type="text" placeholder="Name the City " name="city"
                            Required>
                    </div>

                </div>
                <div class="w-full md:w-full mb-6 md:mb-0 ">
                    <label class="block tracking-wide text-gray-700 text-xs font-bold mb-2 " for="grid-state ">
        Country:
      </label>
                    <div class="relative ">
                        <select class="block appearance-none w-full bg-gray-200 border border-gray-200 text-gray-700 py-3 px-4 pr-8 rounded leading-tight focus:outline-none focus:bg-white focus:border-gray-500 " name="count" id="grid-state ">
          <option>India</option>
          <option>USA</option>
          <option>Britain</option>
        </select>
                        <div class="pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700 ">
                            <svg class="fill-current h-4 w-4 " xmlns="http://www.w3.org/2000/svg " viewBox="0 0 20 20 "><path d="M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z "/></svg>
                        </div>


                    </div>

                </div>
                <p class="text-xs text-red-500 text-left my-3 ">Required fields are marked with an asterisk <abbr title="Required field ">*</abbr></p>

                <div class="mt-5 text-right md:space-x-4 md:block flex flex-col ">
                    <button class="mb-2 md:mb-0 bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-6 rounded " type="submit ">Save & Continue</button>
                    <button class=" mb-2 md:mb-0 bg-transparent hover:bg-blue-500 text-blue-700 font-semibold hover:text-white py-2 px-4 border border-blue-500 hover:border-transparent rounded " onclick="location.href='/'" type="reset"> Cancel </button>
                </div>
            </form>

        </div>
    </div>

    </body>

    </html>

    </html>